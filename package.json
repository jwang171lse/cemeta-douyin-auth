{
  "name": "arco-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "home": "vue-tsc --noEmit && vite --mode home --force",
    "dev": "vue-tsc --noEmit && vite --mode development --force",
    "pro": "vue-tsc --noEmit && vite --mode production",
    "test": "vue-tsc --noEmit && vite --mode test",
    "build:dev": "vue-tsc --noEmit && vite build --mode development",
    "build:pro": "vue-tsc --noEmit && vite build --mode production",
    "build:test": "vue-tsc --noEmit && vite build --mode test",
    "douyin:dev": "yarn build:dev && node ./scripts/publish.mjs development",
    "douyin:pro": "yarn build:pro && node ./scripts/publish.mjs production"
  },
  "dependencies": {
    "@vueuse/core": "^10.4.1",
    "axios": "^1.5.0",
    "base64-encode-file": "^1.0.7",
    "cos-js-sdk-v5": "^1.4.20",
    "jsuri": "^1.3.1",
    "lodash": "^4.17.21",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.6",
    "uid": "^2.0.2",
    "vue": "^3.3.4",
    "vue-router": "4"
  },
  "devDependencies": {
    "@arco-design/web-vue": "^2.51.0",
    "@types/jsuri": "^1.3.32",
    "@types/lodash": "^4.14.198",
    "@types/node": "^20.5.9",
    "@types/nprogress": "^0.2.1",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.2",
    "autoprefixer": "^10.4.15",
    "exec-sh": "^0.4.0",
    "postcss": "^8.4.29",
    "sass": "^1.66.1",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vue-tsc": "^1.8.15"
  }
}
